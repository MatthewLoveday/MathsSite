<% include ../partials/header %>

<div class="container">


    <div class="row">
        <div class="col s12">
            <ul class="tabs">
                <li class="tab col s6"><a href="#users">Users</a></li>
                <li class="tab col s6"><a href="#teachers">Teachers</a></li>
            </ul>
        </div>
        <div id="users" class="col s12">
            <ul class="collection">
                <% for(var i = 0; i < users.length; i++) { %>
                    <% if(users[i].role == "user"){ %>
                        <li class="collection-itme">
                            <span class="title"><%= users[i].username %></span>
                            <p><%= users[i]._id %></p>
                            <a href="/users/<%= userId %>/users/<%= users[i]._id %>" class="secondary-content"><i class="material-icons">remove_red_eye</i></a>
                        </li>
                    <% } %>
                <% } %>
            </ul>
        </div>
        <div id="teachers" class="col s12">
            <ul class="collection">
                <% for(var i = 0; i < users.length; i++) { %>
                    <% if(users[i].role == "teacher"){ %>
                        <li class="collection-itme">
                            <span class="title"><%= users[i].username %></span>
                            <p><%= users[i]._id %></p>
                            <a href="/users/<%= userId %>/users/<%= users[i]._id %>" class="secondary-content"><i class="material-icons">remove_red_eye</i></a>
                        </li>
                    <% } %>
                <% } %>
            </ul>
        </div>
    </div>
</div>



<% include ../partials/footer %>